<select id="getFilmsById" parameterType="java.lang.Long" resultMap="Films"> 
SELECT 
ec.id,
ec.name_films 
FROM 
films AS ec;
WHERE 
    id = #{id}
</select> 
<select id="getAll" resultMap="Films"> 
SELECT 
ec.id,
ec.name_films     
FROM 
films AS ec; 
</select> 
